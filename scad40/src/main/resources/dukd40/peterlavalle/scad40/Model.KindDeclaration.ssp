#import(peterlavalle.scad40.Model)
<%@ val modelKindDeclaration: Model.KindDeclaration %>
<%@ val nephrite: peterlavalle.scad40.Nephrite %>
#match (modelKindDeclaration.declaration)
#case (Model.Script(name, _))
    scad40::duk_ptr<${name}>
#case (Model.Native(name, _))
    scad40::duk_ref<${name}>
#end